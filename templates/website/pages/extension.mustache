  {{#chrome}}
  <div id="install-browser-extension-wrapper">
    {{^loggedUser}}
    <div class="pre-install-extension-wrapper">
      {{#versionA}}
        <button class="btn btn-primary btn-large pre-install-extension" data-target="signup" data-version="A">Sign up to install our Chrome Extension</button> or <a class="pre-install-extension" data-target="login" href="#">Log in</a>
        <div id="skip-signup-install-extension" ><a class="install-extension" href="#">I'd rather try it before giving my email address!</a></div>
      {{/versionA}}
      {{#versionB}}
        <button class="btn btn-success btn-large install-extension" >Install this awesome extension!</button>
      {{/versionB}}
      {{/loggedUser}}
      {{#loggedUser}}
      <button class="btn btn-success btn-large install-extension" >Install this awesome extension!</button>
      {{/loggedUser}}
    </div>
  </div>


  <div id="onboarding">
    <div id="onboarding-overlay">
      <div id="onboarding-wrapper">
        <div id="onboarding-content">
          <div class="row-fluid">
            <div class="span12">
              <div class="signup-container"></div>
              <div class="login-widget">
                <div class="row">
                  <div class="span4 offset4 ">
                    <h1>Login</h1>
                    <span>
                      <div class="alert login-invalid-input" style="display: none;">
                        <button type="button" class="close" data-dismiss="alert">Ã—</button>
                        <small class='login-error-message'></small>
                      </div>
                      <form>
                        <fieldset>
                          <input type="text" class="login-status-email" placeholder="Email goes here" value="{{email}}" name="email"><br><br>
                          <input type="password" class="login-status-password" placeholder="Password" value="{{password}}" name="password"><br><br>
                          <button type="submit" class="btn btn-primary login-status-submit">Log in</button><br><br>
                          <a href="/forgotPassword" class="forgot-password-link"><small>Forgot your password?</small></a><br><br>
                        </fieldset>
                      </form>
                    </span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div id="onboarding-close">
          <button id="onboarding-close-btn"></button>
        </div>
      </div>
    </div>
  </div>
  {{/chrome}}
  {{^chrome}}
  <div class="no-chrome" style="text-align:center;">
    <h3>Looks like you're not using Chrome!</h3> <p>Subscribe to get notified once we have an extension ready for your browser.</p>
