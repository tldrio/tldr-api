<!--Impressing title-->
<header class="jumbotron">
  <h1>Notifications</h1>
</header>

<section id="notifications-container">

  <ul id="notifications-collection">

  {{#notifications}}

  <li class="notification-list-el {{#unseen}}unseen-notif{{/unseen}}" data-id="{{_id}}">
